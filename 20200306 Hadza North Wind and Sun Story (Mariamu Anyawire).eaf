    <TIER LINGUISTIC_TYPE_REF="Translation" PARENT_REF="REF-ID" TIER_ID="Translation">
        <ANNOTATION>
            <REF_ANNOTATION ANNOTATION_ID="a749" ANNOTATION_REF="a698">
                <ANNOTATION_VALUE>When the north wind and the sun were disputing</ANNOTATION_VALUE>
            </REF_ANNOTATION>
        </ANNOTATION>
        <ANNOTATION>
            <REF_ANNOTATION ANNOTATION_ID="a750" ANNOTATION_REF="a699">
                <ANNOTATION_VALUE>who has a lot of strength</ANNOTATION_VALUE>
            </REF_ANNOTATION>
        </ANNOTATION>
        <ANNOTATION>
            <REF_ANNOTATION ANNOTATION_ID="a751" ANNOTATION_REF="a700">
                <ANNOTATION_VALUE>a traveller covered in a heavy shawl (skin) then appeared</ANNOTATION_VALUE>
            </REF_ANNOTATION>
        </ANNOTATION>
        <ANNOTATION>
            <REF_ANNOTATION ANNOTATION_ID="a752" ANNOTATION_REF="a701">
                <ANNOTATION_VALUE>they said</ANNOTATION_VALUE>
            </REF_ANNOTATION>
        </ANNOTATION>
        <ANNOTATION>
            <REF_ANNOTATION ANNOTATION_ID="a753" ANNOTATION_REF="a702">
                <ANNOTATION_VALUE>thus</ANNOTATION_VALUE>
            </REF_ANNOTATION>
        </ANNOTATION>
        <ANNOTATION>
            <REF_ANNOTATION ANNOTATION_ID="a754" ANNOTATION_REF="a703">
                <ANNOTATION_VALUE>the one who goes ahead</ANNOTATION_VALUE>
            </REF_ANNOTATION>
        </ANNOTATION>
        <ANNOTATION>
            <REF_ANNOTATION ANNOTATION_ID="a755" ANNOTATION_REF="a704">
                <ANNOTATION_VALUE>and removes the shawl</ANNOTATION_VALUE>
            </REF_ANNOTATION>
        </ANNOTATION>
        <ANNOTATION>
            <REF_ANNOTATION ANNOTATION_ID="a756" ANNOTATION_REF="a705">
                <ANNOTATION_VALUE>of that person will be said to have strength</ANNOTATION_VALUE>
            </REF_ANNOTATION>
        </ANNOTATION>
        <ANNOTATION>
            <REF_ANNOTATION ANNOTATION_ID="a757" ANNOTATION_REF="a706">
                <ANNOTATION_VALUE>the north wind then gave a lot of strong wind</ANNOTATION_VALUE>
            </REF_ANNOTATION>
        </ANNOTATION>
        <ANNOTATION>
            <REF_ANNOTATION ANNOTATION_ID="a758" ANNOTATION_REF="a707">
                <ANNOTATION_VALUE>but when he blew</ANNOTATION_VALUE>
            </REF_ANNOTATION>
        </ANNOTATION>
        <ANNOTATION>
            <REF_ANNOTATION ANNOTATION_ID="a759" ANNOTATION_REF="a708">
                <ANNOTATION_VALUE>a lot that person closed the shawl tightly</ANNOTATION_VALUE>
            </REF_ANNOTATION>
        </ANNOTATION>
        <ANNOTATION>
            <REF_ANNOTATION ANNOTATION_ID="a760" ANNOTATION_REF="a709">
                <ANNOTATION_VALUE>then the north wind stopped blowing wind</ANNOTATION_VALUE>
            </REF_ANNOTATION>
        </ANNOTATION>
        <ANNOTATION>
            <REF_ANNOTATION ANNOTATION_ID="a761" ANNOTATION_REF="a710">
                <ANNOTATION_VALUE>then the sun shined with heat</ANNOTATION_VALUE>
            </REF_ANNOTATION>
        </ANNOTATION>
        <ANNOTATION>
            <REF_ANNOTATION ANNOTATION_ID="a762" ANNOTATION_REF="a711">
                <ANNOTATION_VALUE>then</ANNOTATION_VALUE>
            </REF_ANNOTATION>
        </ANNOTATION>
        <ANNOTATION>
            <REF_ANNOTATION ANNOTATION_ID="a763" ANNOTATION_REF="a712">
                <ANNOTATION_VALUE>the person removed the shawl</ANNOTATION_VALUE>
            </REF_ANNOTATION>
        </ANNOTATION>
        <ANNOTATION>
            <REF_ANNOTATION ANNOTATION_ID="a764" ANNOTATION_REF="a713">
                <ANNOTATION_VALUE>and so the north wind then knew</ANNOTATION_VALUE>
            </REF_ANNOTATION>
        </ANNOTATION>
        <ANNOTATION>
            <REF_ANNOTATION ANNOTATION_ID="a765" ANNOTATION_REF="a714">
                <ANNOTATION_VALUE>the sun had the most strength among them</ANNOTATION_VALUE>
            </REF_ANNOTATION>
        </ANNOTATION>
